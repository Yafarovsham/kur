#include<fstream>
#include<iostream>
#include<iomanip>
#include<cmath>
using namespace std;

 void PrintMatr(double **mas, int m) 
 {
 int i, j;
 cout << "Matrica:"<< endl;
 for (i = 0; i<m; i++) 
	{
		for (j = 0; j<m; j++)
		cout << setw(5) << mas[i][j] << setw(20) << right;
		cout << endl;
	}
}
void PrintMas(double *X, int m) 
 {
	 cout<<"Vector: "<<endl;
	for(int i=0;i<m;i++)
	cout<<X[i]<<endl;
}

